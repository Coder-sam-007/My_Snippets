	// }
	"boilerplate": {
		"prefix": "sj",
		"body": [
			"#include <iostream>",
			"using namespace std;\n",
			"int main()",
			"{",
			"\t$1",
			"\treturn 0;",
			"}"
		],
		"description": "Boilerplate for C++"
	},
	"input": {
		"prefix": "cin",
		"body": ["cin >> $1;"],
		"description": "Input"
	},
	"output": {
		"prefix": "cout",
		"body": ["cout << $1","<< endl;"],
		"description": "Output"
	}
}